<p id="notice"><%= notice %></p>

<h1>Profesors</h1>

<table class="table table-sm table-hovered">
  <thead>
    <tr>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Telefono</th>
      <th>Correo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profesors.each do |profesor| %>
      <tr>
        <td><%= profesor.nombres %></td>
        <td><%= profesor.apellidos %></td>
        <td><%= profesor.telefono %></td>
        <td><%= profesor.correo %></td>
        <td><%= link_to 'Ver', profesor %></td>
        <td><%= link_to 'Editar', edit_profesor_path(profesor) %></td>
        <td><%= link_to 'Eliminar', profesor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profesor', new_profesor_path %>
