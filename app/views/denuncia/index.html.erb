<p id="notice"><%= notice %></p>

<h1>Denuncia</h1>

<table>
  <thead>
    <tr>
      <th>Asunto</th>
      <th>Texto</th>
      <th>Imagen</th>
      <th>Archivo</th>
      <th>Padre</th>
      <th>Alumno</th>
      <th>Gravedad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @denuncia.each do |denuncium| %>
      <tr>
        <td><%= denuncium.asunto %></td>
        <td><%= denuncium.texto %></td>
        <td><%= denuncium.imagen %></td>
        <td><%= denuncium.archivo %></td>
        <td><%= denuncium.padre %></td>
        <td><%= denuncium.alumno %></td>
        <td><%= denuncium.gravedad %></td>
        <td><%= link_to 'Show', denuncium %></td>
        <td><%= link_to 'Edit', edit_denuncium_path(denuncium) %></td>
        <td><%= link_to 'Destroy', denuncium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Denuncium', new_denuncium_path %>
